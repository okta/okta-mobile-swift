        },
        {
          "inlineContent" : [
            {
              "text" : "The request object returned from this may be customized before presentation, or may be used as-is. For example:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "if let remediation = response.remediations[.enrollAuthenticator],",
            "   let capability = remediation.webAuthnRegistration",
            "{",
            "    let authController = ASAuthorizationController(authorizationRequests: [",
            "        capability.createCredentialRegistrationRequest()",
            "    ])",
            "    authController.delegate = self",
            "    authController.presentationContextProvider = self",
            "    authController.performRequests()",
            "}"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Since Passkeys do not support WebAuthn authenticator attestation, the ",
                  "type" : "text"
                },
