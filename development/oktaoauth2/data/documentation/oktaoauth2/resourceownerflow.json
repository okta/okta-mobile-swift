    },
    {
      "content" : [
        {
          "anchor" : "overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "This simple authentication flow permits a suer to authenticate using a simple username and password. As such, the configuration is straightforward.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Resource Owner authentication does not support MFA or other more secure authentication models, and is not recommended for production applications. Please use the DirectAuth SDK’s DirectAuthenticationFlow class instead.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Important",
          "style" : "important",
          "type" : "aside"
        },
        {
          "anchor" : "Usage",
          "level" : 2,
          "text" : "Usage",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "As an example, we’ll use Swift Concurrency, since these asynchronous methods can be used inline easily.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let client = OAuth2Client(baseURL: URL(string: \"https:\/\/example.okta.com\")!,",
            "                          clientId: \"abc123client\",",
            "                          scopes: \"openid offline_access email profile\")",
            "let flow = client.resourceOwnerFlow()",
            "",
            "\/\/ Authenticate with a username and password.",
            "let token = try await flow.start(username: \"smeagol\", password: \"myprecious\")"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Alternatively, an instance of ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/OktaOAuth2\/documentation\/OktaOAuth2\/ResourceOwnerFlow",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " can be created using one of its initializers. The following example uses the ``ResourceOwnerFlow\/init(issuerURL:clientId:scope:additionalParameters:)` initializer.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let flow = ResourceOwnerFlow(issuerURL: URL(string: \"https:\/\/example.okta.com\")!,",
            "                             clientId: \"abc123client\",",
            "                             scope: \"openid offline_access email profile\")",
            "let token = try await flow.start(username: \"smeagol\", password: \"myprecious\")"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Finally, if you already have an ",
              "type" : "text"
            },
            {
              "code" : "OAuth2Client",
              "type" : "codeVoice"
            },
            {
              "text" : " instance available, you can supply that to the ",
              "type" : "text"
            },
            {
              "code" : "ResourceOwnerFlow\/init(client:)",
              "type" : "codeVoice"
            },
            {
              "text" : " initializer.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let client = OAuth2Client(issuerURL: URL(string: \"https:\/\/example.okta.com\")!,",
            "                          clientId: \"abc123client\",",
            "                          scope: \"openid offline_access email profile\")",
            "let flow = ResourceOwnerFlow(client: client)",
            "let token = try await flow.start(username: \"smeagol\", password: \"myprecious\")"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
