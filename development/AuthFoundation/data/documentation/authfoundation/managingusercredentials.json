ation\/AuthFoundation\/HasClaims","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"HasClaims"}],"url":"\/documentation\/authfoundation\/hasclaims"},"doc://AuthFoundation/documentation/AuthFoundation/TokenInfo":{"role":"symbol","title":"TokenInfo","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"TokenInfo"}],"abstract":[{"type":"text","text":"Introspected token information."}],"identifier":"doc:\/\/AuthFoundation\/documentation\/AuthFoundation\/TokenInfo","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"TokenInfo"}],"url":"\/documentation\/authfoundation\/tokeninfo"},"doc://AuthFoundation/documentation/AuthFoundation/Credential/allIDs":{"role":"symbol","title":"allIDs","fragments":[{"kind":"keyword","text":"static"},{"kind":"text","text":" "},{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"allIDs"},{"kind":"text","text":": ["},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"},{"kind":"text","text":"]"}],"abstract":[{"type":"text","text":"Lists all users currently stored within the user’s application."}],"identifier":"doc:\/\/AuthFoundation\/documentation\/AuthFoundation\/Credential\/allIDs","kind":"symbol","type":"topic","url":"\/documentation\/authfoundation\/credential\/allids"}}}