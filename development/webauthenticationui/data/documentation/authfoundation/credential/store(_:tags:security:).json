      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Token to store.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "token"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Optional developer-assigned tags that can enable you provide additional context to help with differentiating between multiple credentials, or to indicate the intended use for a particular token.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "tags"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Optional collection of security options used to customize how the token is stored.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "options"
        }
      ]
    },
    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Once the token is stored, a ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/com.okta.mobile.webauthenticationui\/documentation\/AuthFoundation\/Credential",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " object is returned that provides conveniences to interact with the token. See ",
              "type" : "text"
            },
            {
              "text" : "doc:ManagingUserCredentials",
              "type" : "text"
            },
            {
              "text" : " for more information.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "The optional arguments ",
              "type" : "text"
            },
            {
              "code" : "tags",
              "type" : "codeVoice"
            },
            {
              "text" : " and ",
              "type" : "text"
            },
            {
              "code" : "security",
              "type" : "codeVoice"
            },
            {
              "text" : " allow you to customize how this token is stored to assist with later retrieval of these objects.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The ",
                  "type" : "text"
                },
                {
                  "identifier" : "doc:\/\/com.okta.mobile.webauthenticationui\/documentation\/AuthFoundation\/Credential\/Security\/standard",
                  "isActive" : true,
                  "type" : "reference"
                },
                {
                  "text" : " static property defines the standard set of security options to use when the ",
                  "type" : "text"
                },
                {
                  "code" : "security",
                  "type" : "codeVoice"
                },
                {
                  "text" : " argument is not provided. To change the default values used when storing credentials, you can change that property to fit your application’s needs.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
