    {
      "content" : [
        {
          "anchor" : "overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Developers can use this to assist in debugging interactions with the Client SDK, and any network operations that are performed on behalf of the user via this SDK.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "This is only available in ",
                  "type" : "text"
                },
                {
                  "code" : "DEBUG",
                  "type" : "codeVoice"
                },
                {
                  "text" : " builds, and should not be used within production applications.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Important",
          "style" : "important",
          "type" : "aside"
        },
        {
          "inlineContent" : [
            {
              "text" : "To use this debug tool, you can either add the shared observer as a delegate to the ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/AuthFoundation\/documentation\/AuthFoundation\/OAuth2Client",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " instance you would like to observe:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let flow = try AuthorizationCodeFlow()",
            "flow.client.add(delegate: DebugAPIRequestObserver.shared)"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Or alternatively you can use the ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/AuthFoundation\/documentation\/AuthFoundation\/DebugAPIRequestObserver\/observeAllOAuth2Clients",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " convenience property to automatically bind to ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/AuthFoundation\/documentation\/AuthFoundation\/OAuth2Client",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " instances as they are initialized.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "func application(",
            "    _ application: UIApplication,",
            "    didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool",
            "{",
            "    \/\/ Application setup",
            "    #if DEBUG",
            "    DebugAPIRequestObserver.shared.observeAllOAuth2Clients = true",
            "    #endif",
            "}"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
